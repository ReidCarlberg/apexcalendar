    <validationRules>
        <fullName>Day_Integer_Range</fullName>
        <active>true</active>
        <errorConditionFormula>or(Day_Int__c &lt; 1,Day_Int__c &gt; 7)</errorConditionFormula>
        <errorMessage>The location hour day must be an integer between 1 and 7.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hour_and_Minutes</fullName>
        <active>true</active>
        <errorConditionFormula>or(Shipping_Open_Hour__c &lt; 0,Shipping_Open_Hour__c &gt; 23,Shipping_Open_Minute__c&lt;0,Shipping_Open_Minute__c&gt;59,Shipping_Close_Hour__c &lt; 0,Shipping_Close_Hour__c &gt; 23,Shipping_Close_Minute__c&lt;0,Shipping_Close_Minute__c&gt;59,Receiving_Open_Hour__c &lt; 0,Receiving_Open_Hour__c &gt; 23,Receiving_Open_Minute__c&lt;0,Receiving_Open_Minute__c&gt;59,Receiving_Close_Hour__c &lt; 0,Receiving_Close_Hour__c &gt; 23,Receiving_Close_Minute__c&lt;0,Receiving_Close_Minute__c&gt;59)</errorConditionFormula>
        <errorMessage>Hours must be between 0 - 23. Minutes must be between 0 - 59.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hours_and_Minutes_Not_Null</fullName>
        <active>true</active>
        <errorConditionFormula>or(and(isnull(Shipping_Open_Hour__c),Shipping_Open_Minute__c &gt; 0),and(isnull(Shipping_Close_Hour__c),Shipping_Close_Minute__c &gt; 0),and(isnull(Receiving_Open_Hour__c),Receiving_Open_Minute__c &gt; 0),and(isnull(Receiving_Close_Hour__c),Receiving_Close_Minute__c &gt; 0))</errorConditionFormula>
        <errorMessage>If you wish to enter a time, you must include the hour.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Receiving_Open_Time_Must_Have_Close_Time</fullName>
        <active>true</active>
        <errorConditionFormula>or(and(not(isnull(Receiving_Open_Hour__c)),isnull(Receiving_Close_Hour__c)),and(isnull(Receiving_Open_Hour__c),not(isnull(Receiving_Close_Hour__c))))</errorConditionFormula>
        <errorMessage>Receiving hours must have open and close times.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shipping_Open_Time_Must_Have_Close_Time</fullName>
        <active>true</active>
